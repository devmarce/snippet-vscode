	//php comentario /**/ marce
	"php comentarios*":{
		"scope": "html,php",
		"prefix": "comment-line php - marce",
		"body": [
			"/* $1 */"
		],
		"description": "php comentario /**/ marce"
	},

	//php comentario /***/ marce
	"php comentarios**":{
		"scope": "html,php",
		"prefix": "comment-**php - marce",
		"body": [
			"/**$1*/"
		],
		"description": "php comentario /***/ marce"
	},

	//php basic
	"estilos de test":{
		"scope": "html,php",
		"prefix": "mmestilos test",
		"body": [
			"<!-- Estilos de test @borrar -->",
			"<style id=\"test-style\" type=\"text/css\"> .$1 {border: 1px solid black;} </style>"
		],
		"description": "zondsa php"
	},

	//php basic
	"var dump":{
		"scope": "html,php",
		"prefix": "vardump - marce",
		"body": [
			"// @borrar - test INI",
			"var_dump($1);$2",
			"// @borrar - test FIN"
		],
		"description": "zondsa php"
	}
